{
    "name": "gaolujie/kiwi",
    "description": "kiwi common application template base on yii2",
    "keywords": [
        "kiwi",
        "yii2"
    ],
    "type": "project",
    "license": "GPL v3.0",
    "support": {
        "issues": "https://github.com/gaolujie/Kiwi/issues?state=open",
        "wiki": "https://github.com/gaolujie/Kiwi/wiki/",
        "source": "https://github.com/gaolujie/Kiwi"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "2amigos/yii2-file-upload-widget": "*",
        "2amigos/yii2-ckeditor-widget": "*",
        "blueimp/jQuery-File-Upload": "*",
        "creocoder/yii2-nested-sets": "*",
        "gilek/yii2-gtreetable": "*",
        "mtdowling/cron-expression": "*",
        "dmstr/yii2-adminlte-asset": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-dynagrid": "*",
        "kartik-v/yii2-builder": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-tree-manager": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
